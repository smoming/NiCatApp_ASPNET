@model IEnumerable<Shipper>

<div class="table-responsive">
    <table id="list" class="table table-bordered table-striped table-hover table-condensed">
        <thead>
            <tr>
                <th>出貨編號</th>
                <th>出貨日期</th>
                <th>買家</th>
                <th>金額</th>
                <th>配送方式</th>
                <th>運費</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var it in Model)
            {
                <tr class="alert-info">
                    <td>@Html.DisplayFor(m => it.TransNo)</td>
                    <td>@Html.DisplayFor(m => it.TradeDate)</td>
                    <td>@Html.DisplayFor(m => it.Buyer)</td>
                    <td>@Html.DisplayFor(m => it.TradeAmount)</td>
                    <td>@*@Html.DisplayFor(m => it.DeliveryType.Name)*@@Html.DisplayFor(m => it.Delivery)</td>
                    <td>@Html.DisplayFor(m => it.Fee)</td>
                </tr>
            }
        </tbody>
    </table>
</div>